			<xsd:element ref="longestrun"/>
			<xsd:element ref="meanqrsdur"/>
			<xsd:element ref="lowventrate"/>
			<xsd:element ref="meanventrate"/>
			<xsd:element ref="highventrate"/>
			<xsd:element ref="ventratestddev"/>
			<xsd:element ref="meanrrint"/>
			<xsd:element ref="atrialrate"/>
			<xsd:element ref="atrialratestddev"/>
