			<xsd:element ref="ramp"/>
			<xsd:element ref="rdur"/>
			<xsd:element ref="samp"/>
			<xsd:element ref="sdur"/>
			<xsd:element ref="rpamp"/>
			<xsd:element ref="rpdur"/>
			<xsd:element ref="spamp"/>
			<xsd:element ref="spdur"/>
			<xsd:element ref="vat"/>
			<xsd:element ref="qrsppk"/>
			<xsd:element ref="qrsdur"/>
